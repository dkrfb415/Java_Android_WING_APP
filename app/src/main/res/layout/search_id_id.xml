<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <EditText android:id="@+id/name_search_re"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:hint="Name"
        android:gravity="center"
        android:textSize="20dp"
        >
    </EditText>

    <EditText android:id="@+id/phone_num_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/name_search_re"
        android:layout_marginTop="30dp"
        android:hint="Phone Num"
        android:textSize="20dp"
        android:gravity="center"
        ></EditText>

    <Button android:id="@+id/searching_id_phone"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/phone_num_search"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="찾기"
        android:textSize="25dp"
        android:background="@android:color/holo_green_dark"
        ></Button>

    <TextView android:id="@+id/search_Result_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/searching_id_phone"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:textSize="20dp"
        ></TextView>


</RelativeLayout>